{
  "expo": {
    "name": "LearnWell",
    "slug": "LearnWell",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "com.hikmahx.LearnWell",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hikmahx.LearnWell"
    },
    "android": {
      "package": "com.hikmahx.LearnWell",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro"
    }
  }
}
